<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.profit.persist.mapper.sys.MenuMapper">

	<select id="selectByParentId" resultType="org.profit.persist.domain.sys.Menu">
		select * from sys_menu where parentId=#{parentId}
    </select>

    <select id="selectAll" resultType="org.profit.persist.domain.sys.Menu">
		select * from sys_menu
    </select>

</mapper>